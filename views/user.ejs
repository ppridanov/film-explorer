<div class="container">
    <h3 class="display-5 text-left mt-3">Hello <%= data.userName %>!!!</h3>
    <p class="display-7 text-left">It's your Account Page. Here you can find your videocollection.</p>
    <div class="row popular-container">
        <% data.results.forEach((el, index) => { %>
        <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12 m-2 card film" filmId="<%= el.id %>">
            <a class="d-flex flex-column  justify-content-fs align-items-center" href="/film/<%= el.id %>"><img
                    src="https://image.tmdb.org/t/p/w185<%= el.poster_path %>">
                <p class="text-dark font-weight-bold mt-1 mb-0" style="min-height: 48px;"><%= el.original_title %></p>
                <p class="text-secondary">
                    <% let overviewSliced = el.overview.slice(0, 50); %>
                    <% if (overviewSliced.length < el.overview.length) { %>
                    <%= overviewSliced %>
                    <% } %>
                </p>
            </a>
            <div class="card-buttons text-center m-3">
                <button class="liked-movie border-0" style="background-color: transparent;">
                    <img src="/images/like-active.svg" width="20px" height="20px">
                </button>
            </div>
        </div>
        <% }) %>

    </div>
</div>